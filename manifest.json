{
  "manifest_version": 2,

  "name": "Tab On Covid",
  "description": "Raise money towards Covid-19 relief with every new tab you open!",
  "version": "1.0",

  "incognito": "split",

  "chrome_url_overrides": {
    "newtab": "public/index.html"
  },

  "browser_action": {
    "default_icon": "assets/img/icon.png"
  },

  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  }
}